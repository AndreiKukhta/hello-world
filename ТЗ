//Первый пункт//
var x=8
if (x>7) {
	document.write("Привет!");
}

//Второй пункт//
var Name="Вячеслав"
if (Name="Вячеслав") {
	document.write("Привет, Вячеслав");
}

//Третий пункт//
var x=0
for(;x<=2-;x+=3){
	document.write(x);
}

